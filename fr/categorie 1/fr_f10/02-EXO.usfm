\c 31  
\p
\v 1 L'\w Éternel|strong="H3068"\w* \w parla|strong="H1696" x-morph="strongMorph:TH8762"\w* à \w Moïse|strong="H4872"\w*, et \w dit|strong="H559" x-morph="strongMorph:TH8800"\w*: 
\v 2  \w Sache|strong="H7200" x-morph="strongMorph:TH8798"\w* que j'ai \w choisi|strong="H7121" x-morph="strongMorph:TH8804"\w*\w |strong="H8034"\w* \w Betsaleel|strong="H1212"\w*, \w fils|strong="H1121"\w* d'\w Uri|strong="H221"\w*, \w fils|strong="H1121"\w* de \w Hur|strong="H2354"\w*, de la \w tribu|strong="H4294"\w* de \w Juda|strong="H3063"\w*. 

\s5
\v 3 Je l'ai \w rempli|strong="H4390" x-morph="strongMorph:TH8762"\w* de l'\w Esprit|strong="H7307"\w* de \w Dieu|strong="H430"\w*, de \w sagesse|strong="H2451"\w*, d'\w intelligence|strong="H8394"\w*, et de \w savoir|strong="H1847"\w* pour toutes sortes d'\w ouvrages|strong="H4399"\w*, 
\v 4 je l'ai rendu capable de \w faire|strong="H2803" x-morph="strongMorph:TH8800"\w* des \w inventions|strong="H4284"\w*, de \w travailler|strong="H6213" x-morph="strongMorph:TH8800"\w* l'\w or|strong="H2091"\w*, l'\w argent|strong="H3701"\w* et l'\w airain|strong="H5178"\w*, 
\v 5 de \w graver|strong="H2799"\w* les \w pierres|strong="H68"\w* à \w enchâsser|strong="H4390" x-morph="strongMorph:TH8763"\w*, de \w travailler|strong="H2799"\w* le \w bois|strong="H6086"\w*, et d'\w exécuter|strong="H6213" x-morph="strongMorph:TH8800"\w* toutes sortes d'\w ouvrages|strong="H4399"\w*. 

\s5
\v 6 Et voici, je lui ai \w donné|strong="H5414" x-morph="strongMorph:TH8804"\w* pour aide \w Oholiab|strong="H171"\w*, \w fils|strong="H1121"\w* d'\w Ahisamac|strong="H294"\w*, de la \w tribu|strong="H4294"\w* de \w Dan|strong="H1835"\w*. J'ai \w mis|strong="H5414" x-morph="strongMorph:TH8804"\w* de l'\w intelligence|strong="H2451"\w* dans l'\w esprit|strong="H3820"\w* de tous ceux qui sont \w habiles|strong="H2450"\w*\w |strong="H3820"\w*, pour qu'ils \w fassent|strong="H6213" x-morph="strongMorph:TH8804"\w* tout ce que je t'ai \w ordonné|strong="H6680" x-morph="strongMorph:TH8765"\w*: 
\v 7 la \w tente|strong="H168"\w* d'\w assignation|strong="H4150"\w*, l'\w arche|strong="H727"\w* du \w témoignage|strong="H5715"\w*, le \w propitiatoire|strong="H3727"\w* qui sera dessus, et tous les \w ustensiles|strong="H3627"\w* de la \w tente|strong="H168"\w*; 
\v 8 la \w table|strong="H7979"\w* et ses \w ustensiles|strong="H3627"\w*, le \w chandelier|strong="H4501"\w* d'or \w pur|strong="H2889"\w* et tous ses \w ustensiles|strong="H3627"\w*, l'\w autel|strong="H4196"\w* des \w parfums|strong="H7004"\w*; 
\v 9 l'\w autel|strong="H4196"\w* des \w holocaustes|strong="H5930"\w* et tous ses \w ustensiles|strong="H3627"\w*, la \w cuve|strong="H3595"\w* avec sa \w base|strong="H3653"\w*; 

\s5
\v 10 les \w vêtements|strong="H899"\w* d'\w office|strong="H8278"\w*, les \w vêtements|strong="H899"\w* \w sacrés|strong="H6944"\w* pour le \w sacrificateur|strong="H3548"\w* \w Aaron|strong="H175"\w*, les \w vêtements|strong="H899"\w* de ses \w fils|strong="H1121"\w* pour les fonctions du \w sacerdoce|strong="H3547" x-morph="strongMorph:TH8763"\w*; 
\v 11 l'\w huile|strong="H8081"\w* d'\w onction|strong="H4888"\w*, et le \w parfum|strong="H7004"\w* \w odoriférant|strong="H5561"\w* pour le \w sanctuaire|strong="H6944"\w*. Ils se \w conformeront|strong="H6213" x-morph="strongMorph:TH8799"\w* à tous les ordres que j'ai \w donnés|strong="H6680" x-morph="strongMorph:TH8765"\w*. 

\s5
\v 12 L'\w Éternel|strong="H3068"\w* \w parla|strong="H559" x-morph="strongMorph:TH8799"\w* à \w Moïse|strong="H4872"\w*, et \w dit|strong="H559" x-morph="strongMorph:TH8800"\w*: 
\v 13  \w Parle|strong="H1696" x-morph="strongMorph:TH8761"\w* aux \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w*, et \w dis|strong="H559" x-morph="strongMorph:TH8800"\w*-leur:\w |strong="H389"\w* Vous ne manquerez pas d'\w observer|strong="H8104" x-morph="strongMorph:TH8799"\w* mes \w sabbats|strong="H7676"\w*, car ce sera entre moi et vous, et parmi vos \w descendants|strong="H1755"\w*, un \w signe|strong="H226"\w* auquel on \w connaîtra|strong="H3045" x-morph="strongMorph:TH8800"\w* que je suis l'\w Éternel|strong="H3068"\w* qui vous \w sanctifie|strong="H6942" x-morph="strongMorph:TH8764"\w*. 
\v 14 Vous \w observerez|strong="H8104" x-morph="strongMorph:TH8804"\w* le \w sabbat|strong="H7676"\w*, car il sera pour vous une chose \w sainte|strong="H6944"\w*. Celui qui le \w profanera|strong="H2490" x-morph="strongMorph:TH8764"\w*, sera puni de \w mort|strong="H4191" x-morph="strongMorph:TH8800"\w*\w |strong="H4191" x-morph="strongMorph:TH8714"\w*; \w celui|strong="H5315"\w* qui \w fera|strong="H6213" x-morph="strongMorph:TH8802"\w* quelque \w ouvrage|strong="H4399"\w* ce jour-là, sera \w retranché|strong="H3772" x-morph="strongMorph:TH8738"\w* du \w milieu|strong="H7130"\w* de son \w peuple|strong="H5971"\w*. 
\v 15 On \w travaillera|strong="H4399"\w*\w |strong="H6213" x-morph="strongMorph:TH8735"\w* \w six|strong="H8337"\w* \w jours|strong="H3117"\w*; mais le \w septième|strong="H7637"\w* jour est le \w sabbat|strong="H7676"\w*, le jour du \w repos|strong="H7677"\w*, \w consacré|strong="H6944"\w* à l'\w Éternel|strong="H3068"\w*. Celui qui \w fera|strong="H6213" x-morph="strongMorph:TH8802"\w* quelque \w ouvrage|strong="H4399"\w* le \w jour|strong="H3117"\w* du \w sabbat|strong="H7676"\w*, sera puni de \w mort|strong="H4191" x-morph="strongMorph:TH8800"\w*\w |strong="H4191" x-morph="strongMorph:TH8714"\w*. 

\s5
\v 16 Les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w* \w observeront|strong="H8104" x-morph="strongMorph:TH8804"\w* le \w sabbat|strong="H7676"\w*, en \w le|strong="H7676"\w* \w célébrant|strong="H6213" x-morph="strongMorph:TH8800"\w*, eux et leurs \w descendants|strong="H1755"\w*, comme une \w alliance|strong="H1285"\w* \w perpétuelle|strong="H5769"\w*. 
\v 17 Ce sera entre moi et les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w* un \w signe|strong="H226"\w* qui devra durer à \w perpétuité|strong="H5769"\w*; car en \w six|strong="H8337"\w* \w jours|strong="H3117"\w* l'\w Éternel|strong="H3068"\w* a \w fait|strong="H6213" x-morph="strongMorph:TH8804"\w* les \w cieux|strong="H8064"\w* et la \w terre|strong="H776"\w*, et le \w septième|strong="H7637"\w* \w jour|strong="H3117"\w* il a \w cessé|strong="H7673" x-morph="strongMorph:TH8804"\w* son oeuvre et il s'est \w reposé|strong="H5314" x-morph="strongMorph:TH8735"\w*. 

\s5
\v 18 Lorsque l'Éternel eut \w achevé|strong="H3615" x-morph="strongMorph:TH8763"\w* de \w parler|strong="H1696" x-morph="strongMorph:TH8763"\w* à \w Moïse|strong="H4872"\w* sur la \w montagne|strong="H2022"\w* de \w Sinaï|strong="H5514"\w*, il lui \w donna|strong="H5414" x-morph="strongMorph:TH8799"\w* les \w deux|strong="H8147"\w* \w tables|strong="H3871"\w* du \w témoignage|strong="H5715"\w*, \w tables|strong="H3871"\w* de \w pierre|strong="H68"\w*, \w écrites|strong="H3789" x-morph="strongMorph:TH8803"\w* du \w doigt|strong="H676"\w* de \w Dieu|strong="H430"\w*. 

\s5
\c 32  
\p
\v 1 Le \w peuple|strong="H5971"\w*, \w voyant|strong="H7200" x-morph="strongMorph:TH8799"\w* que \w Moïse|strong="H4872"\w* \w tardait|strong="H954" x-morph="strongMorph:TH8765"\w* à \w descendre|strong="H3381" x-morph="strongMorph:TH8800"\w* de la \w montagne|strong="H2022"\w*,\w |strong="H5971"\w* s'\w assembla|strong="H6950" x-morph="strongMorph:TH8735"\w* autour d'\w Aaron|strong="H175"\w*, et lui \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: \w Allons|strong="H6965" x-morph="strongMorph:TH8798"\w*! \w fais|strong="H6213" x-morph="strongMorph:TH8798"\w*-nous un \w dieu|strong="H430"\w* qui \w marche|strong="H3212" x-morph="strongMorph:TH8799"\w* \w devant|strong="H6440"\w* nous, car ce \w Moïse|strong="H4872"\w*, cet \w homme|strong="H376"\w* qui nous a fait \w sortir|strong="H5927" x-morph="strongMorph:TH8689"\w* du \w pays|strong="H776"\w* d'\w Égypte|strong="H4714"\w*, nous ne \w savons|strong="H3045" x-morph="strongMorph:TH8804"\w* ce qu'il est devenu. 
\v 2  \w Aaron|strong="H175"\w* leur \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: \w Otez|strong="H6561" x-morph="strongMorph:TH8761"\w* les \w anneaux|strong="H5141"\w* d'\w or|strong="H2091"\w* qui sont aux \w oreilles|strong="H241"\w* de vos \w femmes|strong="H802"\w*, de vos \w fils|strong="H1121"\w* et de vos \w filles|strong="H1323"\w*, et \w apportez|strong="H935" x-morph="strongMorph:TH8685"\w*-les-moi. 

\s5
\v 3 Et \w tous|strong="H5971"\w* \w ôtèrent|strong="H6561" x-morph="strongMorph:TH8691"\w* les \w anneaux|strong="H5141"\w* d'\w or|strong="H2091"\w* qui étaient à leurs \w oreilles|strong="H241"\w*, et ils les \w apportèrent|strong="H935" x-morph="strongMorph:TH8686"\w* à \w Aaron|strong="H175"\w*. 
\v 4 Il les \w reçut|strong="H3947" x-morph="strongMorph:TH8799"\w* de leurs \w mains|strong="H3027"\w*, \w jeta|strong="H6696" x-morph="strongMorph:TH8799"\w* l'or dans un \w moule|strong="H2747"\w*, et \w fit|strong="H6213" x-morph="strongMorph:TH8799"\w* un \w veau|strong="H5695"\w* en \w fonte|strong="H4541"\w*. Et ils \w dirent|strong="H559" x-morph="strongMorph:TH8799"\w*: \w Israël|strong="H3478"\w*! voici ton \w dieu|strong="H430"\w*, qui t'a fait \w sortir|strong="H5927" x-morph="strongMorph:TH8689"\w* du \w pays|strong="H776"\w* d'\w Égypte|strong="H4714"\w*. 

\s5
