\s5
\c 1  
\p
\v 1 Voici les \w noms|strong="H8034"\w* des \w fils|strong="H1121"\w* d'\w Israël|strong="H3478"\w*, \w venus|strong="H935" x-morph="strongMorph:TH8802"\w*\w |strong="H935" x-morph="strongMorph:TH8804"\w* en \w Égypte|strong="H4714"\w* avec \w Jacob|strong="H3290"\w* et la \w famille|strong="H1004"\w* de \w chacun|strong="H376"\w* d'eux: 
\v 2  \w Ruben|strong="H7205"\w*, \w Siméon|strong="H8095"\w*, \w Lévi|strong="H3878"\w*, \w Juda|strong="H3063"\w*, 
\v 3  \w Issacar|strong="H3485"\w*, \w Zabulon|strong="H2074"\w*, \w Benjamin|strong="H1144"\w*, 
\v 4  \w Dan|strong="H1835"\w*, \w Nephthali|strong="H5321"\w*, \w Gad|strong="H1410"\w* et \w Aser|strong="H836"\w*. 
\v 5 Les \w personnes|strong="H5315"\w* \w issues|strong="H3318" x-morph="strongMorph:TH8802"\w*\w |strong="H3409"\w* de \w Jacob|strong="H3290"\w* étaient au nombre de soixante-\w dix|strong="H7657"\w*\w |strong="H5315"\w* en tout. \w Joseph|strong="H3130"\w* était alors en \w Égypte|strong="H4714"\w*. 

\s5
\v 6  \w Joseph|strong="H3130"\w* \w mourut|strong="H4191" x-morph="strongMorph:TH8799"\w*, ainsi que tous ses \w frères|strong="H251"\w* et toute cette \w génération|strong="H1755"\w*-là. 
\v 7 Les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w* furent \w féconds|strong="H6509" x-morph="strongMorph:TH8804"\w* et \w multiplièrent|strong="H8317" x-morph="strongMorph:TH8799"\w*, ils s'\w accrurent|strong="H7235" x-morph="strongMorph:TH8799"\w* et devinrent de \w plus|strong="H3966"\w* en \w plus|strong="H3966"\w* \w puissants|strong="H6105" x-morph="strongMorph:TH8799"\w*. Et le \w pays|strong="H776"\w* en fut \w rempli|strong="H4390" x-morph="strongMorph:TH8735"\w*. 

\s5
\v 8 Il s'\w éleva|strong="H6965" x-morph="strongMorph:TH8799"\w* sur l'\w Égypte|strong="H4714"\w* un \w nouveau|strong="H2319"\w* \w roi|strong="H4428"\w*, qui n'avait point \w connu|strong="H3045" x-morph="strongMorph:TH8804"\w* \w Joseph|strong="H3130"\w*. 
\v 9 Il \w dit|strong="H559" x-morph="strongMorph:TH8799"\w* à son \w peuple|strong="H5971"\w*: Voilà les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w* qui forment un \w peuple|strong="H5971"\w* plus \w nombreux|strong="H7227"\w* et plus \w puissant|strong="H6099"\w* que nous. 
\v 10  \w Allons|strong="H3051" x-morph="strongMorph:TH8798"\w*! montrons-nous \w habiles|strong="H2449" x-morph="strongMorph:TH8691"\w* à son égard; empêchons qu'il ne s'\w accroisse|strong="H7235" x-morph="strongMorph:TH8799"\w*, et que, s'il \w survient|strong="H7122" x-morph="strongMorph:TH8799"\w* une \w guerre|strong="H4421"\w*, \w il|strong="H1931"\w* ne se \w joigne|strong="H3254" x-morph="strongMorph:TH8738"\w* à nos \w ennemis|strong="H8130" x-morph="strongMorph:TH8802"\w*, pour nous \w combattre|strong="H3898" x-morph="strongMorph:TH8738"\w* et \w sortir|strong="H5927" x-morph="strongMorph:TH8804"\w* ensuite du \w pays|strong="H776"\w*. 

\s5
\v 11 Et l'on \w établit|strong="H7760" x-morph="strongMorph:TH8799"\w* sur lui des \w chefs|strong="H8269"\w* de \w corvées|strong="H4522"\w*, afin de l'\w accabler|strong="H6031" x-morph="strongMorph:TH8763"\w* de travaux \w pénibles|strong="H5450"\w*. C'est ainsi qu'il \w bâtit|strong="H1129" x-morph="strongMorph:TH8799"\w* les \w villes|strong="H5892"\w* de \w Pithom|strong="H6619"\w* et de \w Ramsès|strong="H7486"\w*, pour servir de \w magasins|strong="H4543"\w* à \w Pharaon|strong="H6547"\w*. 
\v 12 Mais plus on l'\w accablait|strong="H6031" x-morph="strongMorph:TH8762"\w*, \w plus|strong="H3651"\w* il \w multipliait|strong="H7235" x-morph="strongMorph:TH8799"\w* et s'\w accroissait|strong="H6555" x-morph="strongMorph:TH8799"\w*;\w |strong="H6440"\w* et l'on prit en \w aversion|strong="H6973" x-morph="strongMorph:TH8799"\w* les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w*. 

\s5
\v 13 Alors les \w Égyptiens|strong="H4714"\w* \w réduisirent|strong="H5647"\w*\w \w* les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w* à une \w dure|strong="H6531"\w* \w servitude|strong="H5647" x-morph="strongMorph:TH8686"\w*. 
\v 14 Ils leur rendirent la \w vie|strong="H2416"\w* \w amère|strong="H4843" x-morph="strongMorph:TH8762"\w* par de \w rudes|strong="H7186"\w* \w travaux|strong="H5656"\w* en \w argile|strong="H2563"\w* et en \w briques|strong="H3843"\w*, et par tous les \w ouvrages|strong="H5656"\w* des \w champs|strong="H7704"\w*: et c'était avec \w cruauté|strong="H6531"\w* qu'ils leur \w imposaient|strong="H5647" x-morph="strongMorph:TH8804"\w* toutes ces \w charges|strong="H5656"\w*. 

\s5
\v 15 Le \w roi|strong="H4428"\w* d'\w Égypte|strong="H4714"\w* \w parla|strong="H559" x-morph="strongMorph:TH8799"\w* aussi aux sages-\w femmes|strong="H3205" x-morph="strongMorph:TH8764"\w* des \w Hébreux|strong="H5680"\w*, \w nommées|strong="H8034"\w* l'\w une|strong="H259"\w* \w Schiphra|strong="H8236"\w*, \w et|strong="H8034"\w* l'\w autre|strong="H8145"\w* \w Pua|strong="H6326"\w*. 
\v 16 Il leur \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: Quand vous \w accoucherez|strong="H3205" x-morph="strongMorph:TH8763"\w* les femmes des \w Hébreux|strong="H5680"\w* et que vous les \w verrez|strong="H7200" x-morph="strongMorph:TH8804"\w* sur les \w sièges|strong="H70"\w*, si c'est un \w garçon|strong="H1121"\w*, faites-le \w mourir|strong="H4191" x-morph="strongMorph:TH8689"\w*; si c'est une \w fille|strong="H1323"\w*, laissez-la \w vivre|strong="H2425" x-morph="strongMorph:TH8804"\w*. 
\v 17 Mais les sages-\w femmes|strong="H3205" x-morph="strongMorph:TH8764"\w* \w craignirent|strong="H3372" x-morph="strongMorph:TH8799"\w* \w Dieu|strong="H430"\w*, et ne \w firent|strong="H6213" x-morph="strongMorph:TH8804"\w* point ce que leur avait \w dit|strong="H1696" x-morph="strongMorph:TH8765"\w* le \w roi|strong="H4428"\w* d'\w Égypte|strong="H4714"\w*; elles laissèrent \w vivre|strong="H2421" x-morph="strongMorph:TH8762"\w* les \w enfants|strong="H3206"\w*. 

\s5
\v 18 Le \w roi|strong="H4428"\w* d'\w Égypte|strong="H4714"\w* \w appela|strong="H7121" x-morph="strongMorph:TH8799"\w* les sages-\w femmes|strong="H3205" x-morph="strongMorph:TH8764"\w*, et leur \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: \w Pourquoi|strong="H4069"\w* avez-vous \w agi|strong="H6213" x-morph="strongMorph:TH8804"\w* \w ainsi|strong="H1697"\w*, et avez-vous laissé \w vivre|strong="H2421" x-morph="strongMorph:TH8762"\w* les \w enfants|strong="H3206"\w*? 
\v 19 Les sages-\w femmes|strong="H3205" x-morph="strongMorph:TH8764"\w* \w répondirent|strong="H559" x-morph="strongMorph:TH8799"\w* à \w Pharaon|strong="H6547"\w*: C'est que les \w femmes|strong="H802"\w* des \w Hébreux|strong="H5680"\w* ne sont pas comme les \w Égyptiennes|strong="H4713"\w*; elles sont \w vigoureuses|strong="H2422"\w* et elles \w accouchent|strong="H3205" x-morph="strongMorph:TH8804"\w* \w avant|strong="H2962"\w* l'\w arrivée|strong="H935" x-morph="strongMorph:TH8799"\w* de la sage-\w femme|strong="H3205" x-morph="strongMorph:TH8764"\w*. 

\s5
\v 20  \w Dieu|strong="H430"\w* fit du \w bien|strong="H3190" x-morph="strongMorph:TH8686"\w* aux sages-\w femmes|strong="H3205" x-morph="strongMorph:TH8764"\w*; et le \w peuple|strong="H5971"\w* \w multiplia|strong="H7235" x-morph="strongMorph:TH8799"\w* et devint \w très|strong="H3966"\w* \w nombreux|strong="H6105" x-morph="strongMorph:TH8799"\w*. 
\v 21 Parce que les sages-\w femmes|strong="H3205" x-morph="strongMorph:TH8764"\w* avaient eu la \w crainte|strong="H3372" x-morph="strongMorph:TH8804"\w* de \w Dieu|strong="H430"\w*, Dieu \w fit|strong="H6213" x-morph="strongMorph:TH8799"\w* prospérer leurs \w maisons|strong="H1004"\w*. 
\v 22 Alors \w Pharaon|strong="H6547"\w* donna cet \w ordre|strong="H6680" x-morph="strongMorph:TH8762"\w*\w |strong="H559" x-morph="strongMorph:TH8800"\w* à tout son \w peuple|strong="H5971"\w*: Vous \w jetterez|strong="H7993" x-morph="strongMorph:TH8686"\w* dans le \w fleuve|strong="H2975"\w* tout \w garçon|strong="H1121"\w* qui \w naîtra|strong="H3209"\w*, et vous laisserez \w vivre|strong="H2421" x-morph="strongMorph:TH8762"\w* toutes les \w filles|strong="H1323"\w*. 

\s5
\c 2  
\p
\v 1  \w |strong="H3212" x-morph="strongMorph:TH8799"\w* Un \w homme|strong="H376"\w* de la \w maison|strong="H1004"\w* de \w Lévi|strong="H3878"\w* avait \w pris|strong="H3947" x-morph="strongMorph:TH8799"\w* pour femme une \w fille|strong="H1323"\w* de \w Lévi|strong="H3878"\w*. 
\v 2 Cette \w femme|strong="H802"\w* devint \w enceinte|strong="H2029" x-morph="strongMorph:TH8799"\w* et \w enfanta|strong="H3205" x-morph="strongMorph:TH8799"\w* un \w fils|strong="H1121"\w*. Elle \w vit|strong="H7200" x-morph="strongMorph:TH8799"\w* qu'il était \w beau|strong="H2896"\w*, et elle le \w cacha|strong="H6845" x-morph="strongMorph:TH8799"\w* pendant \w trois|strong="H7969"\w* \w mois|strong="H3391"\w*. 

\s5
\v 3 Ne \w pouvant|strong="H3201" x-morph="strongMorph:TH8804"\w* \w plus|strong="H5750"\w* le \w cacher|strong="H6845" x-morph="strongMorph:TH8687"\w*, elle \w prit|strong="H3947" x-morph="strongMorph:TH8799"\w* une \w caisse|strong="H8392"\w* de \w jonc|strong="H1573"\w*, qu'elle \w enduisit|strong="H2560" x-morph="strongMorph:TH8799"\w* de \w bitume|strong="H2564"\w* et de \w poix|strong="H2203"\w*; elle y \w mit|strong="H7760" x-morph="strongMorph:TH8799"\w* l'\w enfant|strong="H3206"\w*, et le \w déposa|strong="H7760" x-morph="strongMorph:TH8799"\w* parmi les \w roseaux|strong="H5488"\w*, sur le \w bord|strong="H8193"\w* du \w fleuve|strong="H2975"\w*. 
\v 4 La \w soeur|strong="H269"\w* de l'enfant se \w tint|strong="H3320" x-morph="strongMorph:TH8691"\w* à quelque \w distance|strong="H7350"\w*, pour \w savoir|strong="H3045" x-morph="strongMorph:TH8800"\w* ce qui lui \w arriverait|strong="H6213" x-morph="strongMorph:TH8735"\w*. 

\s5
\v 5 La \w fille|strong="H1323"\w* de \w Pharaon|strong="H6547"\w* \w descendit|strong="H3381" x-morph="strongMorph:TH8799"\w* au \w fleuve|strong="H2975"\w* pour se \w baigner|strong="H7364" x-morph="strongMorph:TH8800"\w*, et ses \w compagnes|strong="H5291"\w* se \w promenèrent|strong="H1980" x-morph="strongMorph:TH8802"\w* le \w long|strong="H3027"\w* du \w fleuve|strong="H2975"\w*. Elle \w aperçut|strong="H7200" x-morph="strongMorph:TH8799"\w* la \w caisse|strong="H8392"\w* au \w milieu|strong="H8432"\w* des \w roseaux|strong="H5488"\w*, et elle \w envoya|strong="H7971" x-morph="strongMorph:TH8799"\w* sa \w servante|strong="H519"\w* pour la \w prendre|strong="H3947" x-morph="strongMorph:TH8799"\w*. 
\v 6 Elle l'\w ouvrit|strong="H6605" x-morph="strongMorph:TH8799"\w*, et \w vit|strong="H7200" x-morph="strongMorph:TH8799"\w* l'\w enfant|strong="H3206"\w*: c'était un petit \w garçon|strong="H5288"\w* qui \w pleurait|strong="H1058" x-morph="strongMorph:TH8802"\w*. Elle en eut \w pitié|strong="H2550" x-morph="strongMorph:TH8799"\w*, et elle \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: C'est un \w enfant|strong="H3206"\w* des \w Hébreux|strong="H5680"\w*! 

\s5
\v 7 Alors la \w soeur|strong="H269"\w* de l'enfant \w dit|strong="H559" x-morph="strongMorph:TH8799"\w* à la \w fille|strong="H1323"\w* de \w Pharaon|strong="H6547"\w*: Veux-tu que j'\w aille|strong="H3212" x-morph="strongMorph:TH8799"\w* te \w chercher|strong="H7121" x-morph="strongMorph:TH8804"\w* une \w nourrice|strong="H3243" x-morph="strongMorph:TH8688"\w* parmi les \w femmes|strong="H802"\w* des \w Hébreux|strong="H5680"\w*, pour \w allaiter|strong="H3243" x-morph="strongMorph:TH8686"\w* cet \w enfant|strong="H3206"\w*? 
\v 8  \w Va|strong="H3212" x-morph="strongMorph:TH8798"\w*, lui \w répondit|strong="H559" x-morph="strongMorph:TH8799"\w* la \w fille|strong="H1323"\w* de \w Pharaon|strong="H6547"\w*. Et la jeune \w fille|strong="H5959"\w* \w alla|strong="H3212" x-morph="strongMorph:TH8799"\w* \w chercher|strong="H7121" x-morph="strongMorph:TH8799"\w* la \w mère|strong="H517"\w* de l'\w enfant|strong="H3206"\w*. 

\s5
\v 9 La \w fille|strong="H1323"\w* de \w Pharaon|strong="H6547"\w* lui \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: \w Emporte|strong="H3212" x-morph="strongMorph:TH8685"\w* cet \w enfant|strong="H3206"\w*, et \w allaite|strong="H3243" x-morph="strongMorph:TH8685"\w*-le-moi; je te \w donnerai|strong="H5414" x-morph="strongMorph:TH8799"\w* ton \w salaire|strong="H7939"\w*. La \w femme|strong="H802"\w* \w prit|strong="H3947" x-morph="strongMorph:TH8799"\w* l'\w enfant|strong="H3206"\w*, et l'\w allaita|strong="H5134" x-morph="strongMorph:TH8686"\w*. 
\v 10 Quand \w il|strong="H3206"\w* eut \w grandi|strong="H1431" x-morph="strongMorph:TH8799"\w*, elle l'\w amena|strong="H935" x-morph="strongMorph:TH8686"\w* à la \w fille|strong="H1323"\w* de \w Pharaon|strong="H6547"\w*, et il fut pour elle comme un \w fils|strong="H1121"\w*. Elle lui \w donna|strong="H7121" x-morph="strongMorph:TH8799"\w* le \w nom|strong="H8034"\w* de \w Moïse|strong="H4872"\w*, car, \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*-elle, je l'ai \w retiré|strong="H4871" x-morph="strongMorph:TH8804"\w* des \w eaux|strong="H4325"\w*. 

\s5
\v 11 En ce \w temps|strong="H3117"\w*-là, \w Moïse|strong="H4872"\w*, devenu \w grand|strong="H1431" x-morph="strongMorph:TH8799"\w*, se \w rendit|strong="H3318" x-morph="strongMorph:TH8799"\w* vers ses \w frères|strong="H251"\w*, et fut \w témoin|strong="H7200" x-morph="strongMorph:TH8799"\w* de leurs pénibles \w travaux|strong="H5450"\w*. Il \w vit|strong="H7200" x-morph="strongMorph:TH8799"\w* un \w Égyptien|strong="H4713"\w*\w |strong="H376"\w* qui \w frappait|strong="H5221" x-morph="strongMorph:TH8688"\w* un \w Hébreu|strong="H5680"\w* d'entre ses \w frères|strong="H251"\w*. 
\v 12 Il \w regarda|strong="H6437" x-morph="strongMorph:TH8799"\w* de \w côté|strong="H3541"\w* et d'\w autre|strong="H3541"\w*, et, \w voyant|strong="H7200" x-morph="strongMorph:TH8799"\w* qu'il n'y avait \w personne|strong="H376"\w*, il \w tua|strong="H5221" x-morph="strongMorph:TH8686"\w* l'\w Égyptien|strong="H4713"\w*, et le \w cacha|strong="H2934" x-morph="strongMorph:TH8799"\w* dans le \w sable|strong="H2344"\w*. 

\s5
\v 13 Il \w sortit|strong="H3318" x-morph="strongMorph:TH8799"\w* le \w jour|strong="H3117"\w* \w suivant|strong="H8145"\w*; et voici, \w deux|strong="H8147"\w*\w |strong="H582"\w* \w Hébreux|strong="H5680"\w* se \w querellaient|strong="H5327" x-morph="strongMorph:TH8737"\w*. Il \w dit|strong="H559" x-morph="strongMorph:TH8799"\w* à celui qui avait \w tort|strong="H7563"\w*: Pourquoi \w frappes|strong="H5221" x-morph="strongMorph:TH8686"\w*-tu ton \w prochain|strong="H7453"\w*? 
\v 14 Et cet homme \w répondit|strong="H559" x-morph="strongMorph:TH8799"\w*: Qui \w t|strong="H376"\w*'a \w établi|strong="H7760" x-morph="strongMorph:TH8804"\w* \w chef|strong="H8269"\w* et \w juge|strong="H8199" x-morph="strongMorph:TH8802"\w* sur nous? \w Penses|strong="H559" x-morph="strongMorph:TH8802"\w*-tu me \w tuer|strong="H2026" x-morph="strongMorph:TH8800"\w*, comme tu as \w tué|strong="H2026" x-morph="strongMorph:TH8804"\w* l'\w Égyptien|strong="H4713"\w*? \w Moïse|strong="H4872"\w* eut \w peur|strong="H3372" x-morph="strongMorph:TH8799"\w*, et \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: \w Certainement|strong="H403"\w* la \w chose|strong="H1697"\w* est \w connue|strong="H3045" x-morph="strongMorph:TH8738"\w*. 

\s5
\v 15  \w Pharaon|strong="H6547"\w* \w apprit|strong="H8085" x-morph="strongMorph:TH8799"\w* ce qui s'était \w passé|strong="H1697"\w*, et il \w cherchait|strong="H1245" x-morph="strongMorph:TH8762"\w* à faire \w mourir|strong="H2026" x-morph="strongMorph:TH8800"\w* \w Moïse|strong="H4872"\w*. Mais \w Moïse|strong="H4872"\w* s'\w enfuit|strong="H1272" x-morph="strongMorph:TH8799"\w* de \w devant|strong="H6440"\w* \w Pharaon|strong="H6547"\w*, et il se \w retira|strong="H3427" x-morph="strongMorph:TH8799"\w* dans le \w pays|strong="H776"\w* de \w Madian|strong="H4080"\w*, où il s'\w arrêta|strong="H3427" x-morph="strongMorph:TH8799"\w* près d'un \w puits|strong="H875"\w*. 
\v 16 Le \w sacrificateur|strong="H3548"\w* de \w Madian|strong="H4080"\w* avait \w sept|strong="H7651"\w* \w filles|strong="H1323"\w*. Elles \w vinrent|strong="H935" x-morph="strongMorph:TH8799"\w* \w puiser|strong="H1802" x-morph="strongMorph:TH8799"\w* de l'eau, et elles \w remplirent|strong="H4390" x-morph="strongMorph:TH8762"\w* les \w auges|strong="H7298"\w* pour \w abreuver|strong="H8248" x-morph="strongMorph:TH8687"\w* le \w troupeau|strong="H6629"\w* de leur \w père|strong="H1"\w*. 
\v 17 Les \w bergers|strong="H7462" x-morph="strongMorph:TH8802"\w* \w arrivèrent|strong="H935" x-morph="strongMorph:TH8799"\w*, et les \w chassèrent|strong="H1644" x-morph="strongMorph:TH8762"\w*. Alors \w Moïse|strong="H4872"\w* se \w leva|strong="H6965" x-morph="strongMorph:TH8799"\w*, prit leur \w défense|strong="H3467" x-morph="strongMorph:TH8686"\w*, et fit \w boire|strong="H8248" x-morph="strongMorph:TH8686"\w* leur \w troupeau|strong="H6629"\w*. 

\s5
\v 18 Quand elles furent de \w retour|strong="H935" x-morph="strongMorph:TH8799"\w* auprès de \w Réuel|strong="H7467"\w*, leur \w père|strong="H1"\w*, il \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: \w Pourquoi|strong="H4069"\w* \w revenez|strong="H935" x-morph="strongMorph:TH8800"\w*-vous si \w tôt|strong="H4116" x-morph="strongMorph:TH8765"\w* aujourd'\w hui|strong="H3117"\w*? 
\v 19 Elles \w répondirent|strong="H559" x-morph="strongMorph:TH8799"\w*: Un \w Égyptien|strong="H4713"\w*\w |strong="H376"\w* nous a \w délivrées|strong="H5337" x-morph="strongMorph:TH8689"\w* de la \w main|strong="H3027"\w* des \w bergers|strong="H7462" x-morph="strongMorph:TH8802"\w*,\w |strong="H1802" x-morph="strongMorph:TH8800"\w* et même il nous a \w puisé|strong="H1802" x-morph="strongMorph:TH8804"\w* de l'eau, et a fait \w boire|strong="H8248" x-morph="strongMorph:TH8686"\w* le \w troupeau|strong="H6629"\w*. 
