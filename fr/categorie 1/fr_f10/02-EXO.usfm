\s5
\v 28  \w Lorsque|strong="H3117"\w* l'\w Éternel|strong="H3068"\w* \w parla|strong="H1696" x-morph="strongMorph:TH8763"\w* à \w Moïse|strong="H4872"\w* dans le \w pays|strong="H776"\w* d'\w Égypte|strong="H4714"\w*, 
\v 29 l'\w Éternel|strong="H3068"\w* \w dit|strong="H1696" x-morph="strongMorph:TH8762"\w*\w |strong="H559" x-morph="strongMorph:TH8800"\w* à \w Moïse|strong="H4872"\w*: Je suis l'\w Éternel|strong="H3068"\w*. \w Dis|strong="H1696" x-morph="strongMorph:TH8761"\w* à \w Pharaon|strong="H6547"\w*, \w roi|strong="H4428"\w* d'\w Égypte|strong="H4714"\w*, tout ce que je te \w dis|strong="H1696" x-morph="strongMorph:TH8802"\w*. 
\v 30 Et \w Moïse|strong="H4872"\w* \w répondit|strong="H559" x-morph="strongMorph:TH8799"\w* en \w présence|strong="H6440"\w* de l'\w Éternel|strong="H3068"\w*: Voici, je n'ai pas la \w parole|strong="H8193"\w* \w facile|strong="H6189"\w*; comment \w Pharaon|strong="H6547"\w* m'\w écouterait|strong="H8085" x-morph="strongMorph:TH8799"\w*-il? 

\s5
\c 7  
\p
\v 1 L'\w Éternel|strong="H3068"\w* \w dit|strong="H559" x-morph="strongMorph:TH8799"\w* à \w Moïse|strong="H4872"\w*: \w Vois|strong="H7200" x-morph="strongMorph:TH8798"\w*, je te \w fais|strong="H5414" x-morph="strongMorph:TH8804"\w* \w Dieu|strong="H430"\w* pour \w Pharaon|strong="H6547"\w*: et \w Aaron|strong="H175"\w*, ton \w frère|strong="H251"\w*, sera ton \w prophète|strong="H5030"\w*. 
\v 2 Toi, tu \w diras|strong="H1696" x-morph="strongMorph:TH8762"\w* tout ce que je t'\w ordonnerai|strong="H6680" x-morph="strongMorph:TH8762"\w*; et \w Aaron|strong="H175"\w*, ton \w frère|strong="H251"\w*, \w parlera|strong="H1696" x-morph="strongMorph:TH8762"\w* à \w Pharaon|strong="H6547"\w*, pour qu'il laisse \w aller|strong="H7971" x-morph="strongMorph:TH8765"\w* les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w* hors de son \w pays|strong="H776"\w*. 

\s5
\v 3 Et moi, j'\w endurcirai|strong="H7185" x-morph="strongMorph:TH8686"\w* le \w coeur|strong="H3820"\w* de \w Pharaon|strong="H6547"\w*, et je \w multiplierai|strong="H7235" x-morph="strongMorph:TH8689"\w* mes \w signes|strong="H226"\w* et mes \w miracles|strong="H4159"\w* dans le \w pays|strong="H776"\w* d'\w Égypte|strong="H4714"\w*. 
\v 4  \w Pharaon|strong="H6547"\w* ne vous écoutera \w point|strong="H8085" x-morph="strongMorph:TH8799"\w*. Je \w mettrai|strong="H5414" x-morph="strongMorph:TH8804"\w* ma \w main|strong="H3027"\w* sur l'\w Égypte|strong="H4714"\w*, et je ferai \w sortir|strong="H3318" x-morph="strongMorph:TH8689"\w* du \w pays|strong="H776"\w* d'\w Égypte|strong="H4714"\w* mes \w armées|strong="H6635"\w*, mon \w peuple|strong="H5971"\w*, les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w*, par de \w grands|strong="H1419"\w* \w jugements|strong="H8201"\w*. 
\v 5 Les \w Égyptiens|strong="H4714"\w* \w connaîtront|strong="H3045" x-morph="strongMorph:TH8804"\w* que je suis l'\w Éternel|strong="H3068"\w*, lorsque j'\w étendrai|strong="H5186" x-morph="strongMorph:TH8800"\w* ma \w main|strong="H3027"\w* sur l'\w Égypte|strong="H4714"\w*, et que je ferai \w sortir|strong="H3318" x-morph="strongMorph:TH8689"\w* du \w milieu|strong="H8432"\w* d'eux les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w*. 

\s5
\v 6  \w Moïse|strong="H4872"\w* et \w Aaron|strong="H175"\w* \w firent|strong="H6213" x-morph="strongMorph:TH8799"\w* ce que l'\w Éternel|strong="H3068"\w* leur avait \w ordonné|strong="H6680" x-morph="strongMorph:TH8765"\w*; ils \w firent|strong="H6213" x-morph="strongMorph:TH8804"\w* ainsi. 
\v 7  \w Moïse|strong="H4872"\w* était \w âgé|strong="H1121"\w* de quatre-\w vingts|strong="H8084"\w* \w ans|strong="H8141"\w*,\w |strong="H1121"\w* et \w Aaron|strong="H175"\w* de quatre-\w vingt|strong="H8084"\w*-\w trois|strong="H7969"\w* \w ans|strong="H8141"\w*, lorsqu'ils \w parlèrent|strong="H1696" x-morph="strongMorph:TH8763"\w* à \w Pharaon|strong="H6547"\w*. 

\s5
\v 8 L'\w Éternel|strong="H3068"\w* \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*\w |strong="H559" x-morph="strongMorph:TH8800"\w* à \w Moïse|strong="H4872"\w* et à \w Aaron|strong="H175"\w*: 
\v 9 Si \w Pharaon|strong="H6547"\w* vous \w parle|strong="H1696" x-morph="strongMorph:TH8762"\w*, et vous \w dit|strong="H559" x-morph="strongMorph:TH8800"\w*: \w Faites|strong="H5414" x-morph="strongMorph:TH8798"\w* un \w miracle|strong="H4159"\w*! tu \w diras|strong="H559" x-morph="strongMorph:TH8804"\w* à \w Aaron|strong="H175"\w*: \w Prends|strong="H3947" x-morph="strongMorph:TH8798"\w* ta \w verge|strong="H4294"\w*, et \w jette|strong="H7993" x-morph="strongMorph:TH8685"\w*-la \w devant|strong="H6440"\w* \w Pharaon|strong="H6547"\w*. Elle deviendra un \w serpent|strong="H8577"\w*. 
\v 10  \w Moïse|strong="H4872"\w* et \w Aaron|strong="H175"\w* \w allèrent|strong="H935" x-morph="strongMorph:TH8799"\w* auprès de \w Pharaon|strong="H6547"\w*, et ils \w firent|strong="H6213" x-morph="strongMorph:TH8799"\w* ce que l'\w Éternel|strong="H3068"\w* avait \w ordonné|strong="H6680" x-morph="strongMorph:TH8765"\w*. \w Aaron|strong="H175"\w* \w jeta|strong="H7993" x-morph="strongMorph:TH8686"\w* sa \w verge|strong="H4294"\w* \w devant|strong="H6440"\w* \w Pharaon|strong="H6547"\w* et \w devant|strong="H6440"\w* ses \w serviteurs|strong="H5650"\w*; et elle devint un \w serpent|strong="H8577"\w*. 

\s5
\v 11 Mais \w Pharaon|strong="H6547"\w* \w appela|strong="H7121" x-morph="strongMorph:TH8799"\w* des \w sages|strong="H2450"\w* et des \w enchanteurs|strong="H3784" x-morph="strongMorph:TH8764"\w*; et les \w magiciens|strong="H2748"\w* d'\w Égypte|strong="H4714"\w*, eux aussi, en \w firent|strong="H6213" x-morph="strongMorph:TH8799"\w* \w autant|strong="H3651"\w* par leurs \w enchantements|strong="H3858"\w*. 
\v 12 Ils \w jetèrent|strong="H7993" x-morph="strongMorph:TH8686"\w* \w tous|strong="H376"\w* leurs \w verges|strong="H4294"\w*, et elles devinrent des \w serpents|strong="H8577"\w*. Et la \w verge|strong="H4294"\w* d'\w Aaron|strong="H175"\w* \w engloutit|strong="H1104" x-morph="strongMorph:TH8799"\w* leurs \w verges|strong="H4294"\w*. 
\v 13 Le \w coeur|strong="H3820"\w* de \w Pharaon|strong="H6547"\w* s'\w endurcit|strong="H2388" x-morph="strongMorph:TH8799"\w*, et il n'\w écouta|strong="H8085" x-morph="strongMorph:TH8804"\w* point Moïse et Aaron selon ce que l'\w Éternel|strong="H3068"\w* avait \w dit|strong="H1696" x-morph="strongMorph:TH8765"\w*. 

\s5
\v 14 L'\w Éternel|strong="H3068"\w* \w dit|strong="H559" x-morph="strongMorph:TH8799"\w* à \w Moïse|strong="H4872"\w*: \w Pharaon|strong="H6547"\w* a le \w coeur|strong="H3820"\w* \w endurci|strong="H3515"\w*; il \w refuse|strong="H3985" x-morph="strongMorph:TH8765"\w* de laisser \w aller|strong="H7971" x-morph="strongMorph:TH8763"\w* le \w peuple|strong="H5971"\w*. 
\v 15  \w Va|strong="H3212" x-morph="strongMorph:TH8798"\w* vers \w Pharaon|strong="H6547"\w* dès le \w matin|strong="H1242"\w*; il \w sortira|strong="H3318" x-morph="strongMorph:TH8802"\w* pour aller près de l'\w eau|strong="H4325"\w*, et tu te \w présenteras|strong="H5324" x-morph="strongMorph:TH8738"\w* \w devant|strong="H7125" x-morph="strongMorph:TH8800"\w* lui au \w bord|strong="H8193"\w* du \w fleuve|strong="H2975"\w*. Tu \w prendras|strong="H3947" x-morph="strongMorph:TH8799"\w* à ta \w main|strong="H3027"\w* la \w verge|strong="H4294"\w* qui a été \w changée|strong="H2015" x-morph="strongMorph:TH8738"\w* en \w serpent|strong="H5175"\w*, 

\s5
\v 16 et tu \w diras|strong="H559" x-morph="strongMorph:TH8804"\w* à Pharaon: L'\w Éternel|strong="H3068"\w*, le \w Dieu|strong="H430"\w* des \w Hébreux|strong="H5680"\w*, m'a \w envoyé|strong="H7971" x-morph="strongMorph:TH8804"\w* auprès de toi, pour te \w dire|strong="H559" x-morph="strongMorph:TH8800"\w*: Laisse \w aller|strong="H7971" x-morph="strongMorph:TH8761"\w* mon \w peuple|strong="H5971"\w*, afin qu'il me \w serve|strong="H5647" x-morph="strongMorph:TH8799"\w* dans le \w désert|strong="H4057"\w*. Et voici, jusqu'à \w présent|strong="H3541"\w* tu n'as point \w écouté|strong="H8085" x-morph="strongMorph:TH8804"\w*. 
\v 17 Ainsi \w parle|strong="H559" x-morph="strongMorph:TH8804"\w* l'\w Éternel|strong="H3068"\w*: À ceci tu \w connaîtras|strong="H3045" x-morph="strongMorph:TH8799"\w* que je suis l'\w Éternel|strong="H3068"\w*. Je vais \w frapper|strong="H5221" x-morph="strongMorph:TH8688"\w* les \w eaux|strong="H4325"\w* du \w fleuve|strong="H2975"\w* avec la \w verge|strong="H4294"\w* qui est dans ma \w main|strong="H3027"\w*; et elles seront \w changées|strong="H2015" x-morph="strongMorph:TH8738"\w* en \w sang|strong="H1818"\w*. 
\v 18 Les \w poissons|strong="H1710"\w* qui sont dans le \w fleuve|strong="H2975"\w* \w périront|strong="H4191" x-morph="strongMorph:TH8799"\w*, le \w fleuve|strong="H2975"\w* se \w corrompra|strong="H887" x-morph="strongMorph:TH8804"\w*, et les \w Égyptiens|strong="H4714"\w* s'\w efforceront|strong="H3811" x-morph="strongMorph:TH8738"\w* en vain de \w boire|strong="H8354" x-morph="strongMorph:TH8800"\w* l'\w eau|strong="H4325"\w* du \w fleuve|strong="H2975"\w*. 

\s5
\v 19 L'\w Éternel|strong="H3068"\w* \w dit|strong="H559" x-morph="strongMorph:TH8799"\w* à \w Moïse|strong="H4872"\w*: \w Dis|strong="H559" x-morph="strongMorph:TH8798"\w* à \w Aaron|strong="H175"\w*: \w Prends|strong="H3947" x-morph="strongMorph:TH8798"\w* ta \w verge|strong="H4294"\w*, et \w étends|strong="H5186" x-morph="strongMorph:TH8798"\w* ta \w main|strong="H3027"\w* sur les \w eaux|strong="H4325"\w* des \w Égyptiens|strong="H4714"\w*, sur leurs \w rivières|strong="H5104"\w*, sur leurs \w ruisseaux|strong="H2975"\w*, sur leurs \w étangs|strong="H98"\w*, et sur tous leurs \w amas|strong="H4723"\w* d'\w eaux|strong="H4325"\w*. Elles deviendront du \w sang|strong="H1818"\w*: et il y aura du \w sang|strong="H1818"\w* dans tout le \w pays|strong="H776"\w* d'\w Égypte|strong="H4714"\w*, dans les vases de \w bois|strong="H6086"\w* et dans les vases de \w pierre|strong="H68"\w*. 

\s5
\v 20  \w Moïse|strong="H4872"\w* et \w Aaron|strong="H175"\w* \w firent|strong="H6213" x-morph="strongMorph:TH8799"\w* ce que l'\w Éternel|strong="H3068"\w* avait \w ordonné|strong="H6680" x-morph="strongMorph:TH8765"\w*. Aaron \w leva|strong="H7311" x-morph="strongMorph:TH8686"\w* la \w verge|strong="H4294"\w*, et il \w frappa|strong="H5221" x-morph="strongMorph:TH8686"\w* les \w eaux|strong="H4325"\w* qui étaient dans le \w fleuve|strong="H2975"\w*, sous les \w yeux|strong="H5869"\w* de \w Pharaon|strong="H6547"\w* et sous les \w yeux|strong="H5869"\w* de ses \w serviteurs|strong="H5650"\w*; et toutes les \w eaux|strong="H4325"\w* du \w fleuve|strong="H2975"\w* furent \w changées|strong="H2015" x-morph="strongMorph:TH8735"\w* en \w sang|strong="H1818"\w*. 
\v 21 Les \w poissons|strong="H1710"\w* qui étaient dans le \w fleuve|strong="H2975"\w* \w périrent|strong="H4191" x-morph="strongMorph:TH8804"\w*, le \w fleuve|strong="H2975"\w* se \w corrompit|strong="H887" x-morph="strongMorph:TH8799"\w*, les \w Égyptiens|strong="H4714"\w* ne \w pouvaient|strong="H3201" x-morph="strongMorph:TH8804"\w* plus \w boire|strong="H8354" x-morph="strongMorph:TH8800"\w* l'\w eau|strong="H4325"\w* du \w fleuve|strong="H2975"\w*, et il y eut du \w sang|strong="H1818"\w* dans tout le \w pays|strong="H776"\w* d'\w Égypte|strong="H4714"\w*. 
\v 22 Mais les \w magiciens|strong="H2748"\w* d'\w Égypte|strong="H4714"\w* en \w firent|strong="H6213" x-morph="strongMorph:TH8799"\w* autant par leurs \w enchantements|strong="H3909"\w*. Le \w coeur|strong="H3820"\w* de \w Pharaon|strong="H6547"\w* s'\w endurcit|strong="H2388" x-morph="strongMorph:TH8799"\w*, et il n'\w écouta|strong="H8085" x-morph="strongMorph:TH8804"\w* point Moïse et Aaron, selon ce que l'\w Éternel|strong="H3068"\w* avait \w dit|strong="H1696" x-morph="strongMorph:TH8765"\w*. 

\s5
\v 23  \w Pharaon|strong="H6547"\w* s'en \w retourna|strong="H6437" x-morph="strongMorph:TH8799"\w*, et \w alla|strong="H935" x-morph="strongMorph:TH8799"\w* dans sa \w maison|strong="H1004"\w*; et il ne \w prit|strong="H7896" x-morph="strongMorph:TH8804"\w* pas même à \w coeur|strong="H3820"\w* ces \w choses|strong="H2063"\w*. 
\v 24 Tous les \w Égyptiens|strong="H4714"\w* \w creusèrent|strong="H2658" x-morph="strongMorph:TH8799"\w* aux \w environs|strong="H5439"\w* du \w fleuve|strong="H2975"\w*, pour trouver de l'\w eau|strong="H4325"\w* à \w boire|strong="H8354" x-morph="strongMorph:TH8800"\w*; car ils ne \w pouvaient|strong="H3201" x-morph="strongMorph:TH8804"\w* \w boire|strong="H8354" x-morph="strongMorph:TH8800"\w* de l'\w eau|strong="H4325"\w* du \w fleuve|strong="H2975"\w*. 
\v 25 Il s'\w écoula|strong="H4390" x-morph="strongMorph:TH8735"\w* \w sept|strong="H7651"\w* \w jours|strong="H3117"\w*, \w après|strong="H310"\w* que l'\w Éternel|strong="H3068"\w* eut \w frappé|strong="H5221" x-morph="strongMorph:TH8687"\w* le \w fleuve|strong="H2975"\w*. 

\s5
\c 8  
\p
\v 1  L'\w Éternel|strong="H3068"\w* \w dit|strong="H559" x-morph="strongMorph:TH8799"\w* à \w Moïse|strong="H4872"\w*: \w Va|strong="H935" x-morph="strongMorph:TH8798"\w* vers \w Pharaon|strong="H6547"\w*, et tu lui \w diras|strong="H559" x-morph="strongMorph:TH8804"\w*: Ainsi \w parle|strong="H559" x-morph="strongMorph:TH8804"\w* l'\w Éternel|strong="H3068"\w*: Laisse \w aller|strong="H7971" x-morph="strongMorph:TH8761"\w* mon \w peuple|strong="H5971"\w*, afin qu'il me \w serve|strong="H5647" x-morph="strongMorph:TH8799"\w*. 
\v 2  Si tu \w refuses|strong="H3986"\w* de le laisser \w aller|strong="H7971" x-morph="strongMorph:TH8763"\w*, je vais \w frapper|strong="H5062" x-morph="strongMorph:TH8802"\w* par des \w grenouilles|strong="H6854"\w* toute l'\w étendue|strong="H1366"\w* de ton pays. 
\v 3  Le \w fleuve|strong="H2975"\w* \w fourmillera|strong="H8317" x-morph="strongMorph:TH8804"\w* de \w grenouilles|strong="H6854"\w*; elles \w monteront|strong="H5927" x-morph="strongMorph:TH8804"\w*, et elles \w entreront|strong="H935" x-morph="strongMorph:TH8804"\w* dans ta \w maison|strong="H1004"\w*, dans ta \w chambre|strong="H2315"\w* à \w coucher|strong="H4904"\w* et dans ton \w lit|strong="H4296"\w*, dans la \w maison|strong="H1004"\w* de tes \w serviteurs|strong="H5650"\w* et dans celles de ton \w peuple|strong="H5971"\w*, dans tes \w fours|strong="H8574"\w* et dans tes \w pétrins|strong="H4863"\w*. 
\v 4  Les \w grenouilles|strong="H6854"\w* \w monteront|strong="H5927" x-morph="strongMorph:TH8799"\w* sur toi, sur ton \w peuple|strong="H5971"\w*, et sur tous tes \w serviteurs|strong="H5650"\w*. 

\s5
\v 5  L'\w Éternel|strong="H3068"\w* \w dit|strong="H559" x-morph="strongMorph:TH8799"\w* à \w Moïse|strong="H4872"\w*: \w Dis|strong="H559" x-morph="strongMorph:TH8798"\w* à \w Aaron|strong="H175"\w*: \w Étends|strong="H5186" x-morph="strongMorph:TH8798"\w* ta \w main|strong="H3027"\w* avec ta \w verge|strong="H4294"\w* sur les \w rivières|strong="H5104"\w*, sur les \w ruisseaux|strong="H2975"\w* et sur les \w étangs|strong="H98"\w*, et fais \w monter|strong="H5927" x-morph="strongMorph:TH8685"\w* les \w grenouilles|strong="H6854"\w* sur le \w pays|strong="H776"\w* d'\w Égypte|strong="H4714"\w*. 
\v 6  \w Aaron|strong="H175"\w* \w étendit|strong="H5186" x-morph="strongMorph:TH8799"\w* sa \w main|strong="H3027"\w* sur les \w eaux|strong="H4325"\w* de l'\w Égypte|strong="H4714"\w*; et les \w grenouilles|strong="H6854"\w* \w montèrent|strong="H5927" x-morph="strongMorph:TH8799"\w* et \w couvrirent|strong="H3680" x-morph="strongMorph:TH8762"\w* le \w pays|strong="H776"\w* d'\w Égypte|strong="H4714"\w*. 
\v 7  Mais les \w magiciens|strong="H2748"\w* en \w firent|strong="H6213" x-morph="strongMorph:TH8799"\w* autant par leurs \w enchantements|strong="H3909"\w*. Ils firent \w monter|strong="H5927" x-morph="strongMorph:TH8686"\w* les \w grenouilles|strong="H6854"\w* sur le \w pays|strong="H776"\w* d'\w Égypte|strong="H4714"\w*. 

\s5
\v 8  \w Pharaon|strong="H6547"\w* \w appela|strong="H7121" x-morph="strongMorph:TH8799"\w* \w Moïse|strong="H4872"\w* et \w Aaron|strong="H175"\w*, et \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: \w Priez|strong="H6279" x-morph="strongMorph:TH8685"\w* l'\w Éternel|strong="H3068"\w*, afin qu'il \w éloigne|strong="H5493" x-morph="strongMorph:TH8686"\w* les \w grenouilles|strong="H6854"\w* de moi et de mon \w peuple|strong="H5971"\w*; et je laisserai \w aller|strong="H7971" x-morph="strongMorph:TH8762"\w* le \w peuple|strong="H5971"\w*, pour qu'il offre des \w sacrifices|strong="H2076" x-morph="strongMorph:TH8799"\w* à l'\w Éternel|strong="H3068"\w*. 
\v 9  \w Moïse|strong="H4872"\w* \w dit|strong="H559" x-morph="strongMorph:TH8799"\w* à \w Pharaon|strong="H6547"\w*: \w Glorifie|strong="H6286" x-morph="strongMorph:TH8690"\w*-toi sur moi! Pour \w quand|strong="H4970"\w* \w prierai|strong="H6279" x-morph="strongMorph:TH8686"\w*-je l'Éternel en ta faveur, en faveur de tes \w serviteurs|strong="H5650"\w* et de ton \w peuple|strong="H5971"\w*, afin qu'il \w retire|strong="H3772" x-morph="strongMorph:TH8687"\w* les \w grenouilles|strong="H6854"\w* loin de toi et de tes \w maisons|strong="H1004"\w*? Il n'en \w restera|strong="H7604" x-morph="strongMorph:TH8735"\w* que dans le \w fleuve|strong="H2975"\w*. 

\s5
\v 10  Il \w répondit|strong="H559" x-morph="strongMorph:TH8799"\w*: Pour \w demain|strong="H4279"\w*. Et Moïse \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: Il en sera \w ainsi|strong="H1697"\w*, afin que tu \w saches|strong="H3045" x-morph="strongMorph:TH8799"\w* que nul n'est semblable à l'\w Éternel|strong="H3068"\w*, notre \w Dieu|strong="H430"\w*. 
\v 11  Les \w grenouilles|strong="H6854"\w* s'\w éloigneront|strong="H5493" x-morph="strongMorph:TH8804"\w* de toi et de tes \w maisons|strong="H1004"\w*, de tes \w serviteurs|strong="H5650"\w* et de ton \w peuple|strong="H5971"\w*; il n'en \w restera|strong="H7604" x-morph="strongMorph:TH8735"\w* que dans le \w fleuve|strong="H2975"\w*. 
